using System;

public class First
{
    public static void Main(string[] args)
    {
		int characterAge;
		characterAge = 35;
		bool isMale = false;
		string characterName = "Ben";
		
        Console.WriteLine("Hello " + characterName + "," );
		
		if(isMale && characterAge > 20)
		{
			Console.WriteLine("you're looking kinda old sir.");
		}else if(isMale && characterAge < 20)
		{
				Console.WriteLine("you bright youngster lad!");
		}else
		{	
			Console.WriteLine("you've changed.");
		}
     
    }    
}

using System;

public class First
{
    public static void Main(string[] args)
    {
		Console.WriteLine(GetMax(10,2,40));
		
    }    
	
	static int GetMax(int num1, int num2, int num3)
	{
		int result;
		if(num1 >= num2 && num1 >= num3)
		{ 
			result = num1;
			
		}else if ( num2 >= num1 && num2 >= num3)
		{
				result = num2;
			}else
		{ 
			result = num3;
		}
		
		return result;
	}
}

using System;
					
namespace Kizzy
{
	public class Program
	{
		public static void Main()
		{
			int index = 6;
			do
			{
				Console.WriteLine(index);
				index++;
			} while (index <= 5);
		
	
		}
	}
}

using System;
					
namespace Kizzy
{
	public class Program
	{
		public static void Main()
		{
			
			Console.WriteLine("Hello! " + cube(5));
		}
		
		static int cube(int num)
		{
			int result = num *num*num;
			return result;
		}
	}
