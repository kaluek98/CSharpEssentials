using System;

namespace kizzy
{
					
public class Program
{
	public void Main()
	{
		string[] dogs = {"Poodle", "German Sheppard", "Doxen", "French Bulldog", "Golden Retriever"};
		foreach (string g in dogs)
		{
		Console.WriteLine(g);
		}
	}
}
}
using System;

namespace kizzy
{
					
public class Program
{
	public void Main()
	{
		string[] dogs = {"Poodle", "German Sheppard", "Doxen", "French Bulldog", "Golden Retriever"};
		Console.WriteLine(dogs.Length);
		foreach (string g in dogs)
		{
		Console.WriteLine("I want a " + g + ".");
		}
		
	}
}
}
using System;

namespace kizzy
{
					
public class Program
{
	public void Main()
	{
		string[] playerClasses = {"Goblin", "Goblin", "Goblin", "Dog"};
		playerClasses[0] = "Orc";
		
		for (var i = 1;i< playerClasses.Length; i++)
		
		{
			playerClasses[i] = "Human";
		}
		
		foreach(var playerClass in playerClasses)
		{ Console.WriteLine(playerClass);
		}
		
	}
}
}

using System;

namespace kizzy
{
					
public class Program
{
	public static void Main()
	{
		item[] itemObjs = {new item(), new item(), new item()};
		itemObjs[1].itemNam = "Stick";
		itemObjs[2].itemNam = "Rope";
		
		for(var i = 1; i < itemObjs.Length;i++){
			itemObjs[i].itemValue = 2;
		}
		
		foreach(var piece in itemObjs){
			Console.WriteLine("You have a " + piece.itemNam);
			Console.WriteLine("worth " +  piece.itemValue + "$");
		}
	}
}
public class item
{
	public string itemNam = "Potion";
	public int itemValue = 7;
}
}
using System;

namespace kizzy
{
					
public class Program
{
	public static void Main()
	{
		friend[] friends = { new friend() , new friend(), new friend() , new friend() };
		Console.WriteLine("Which friend do you want to call?");
		friends[0].friendNam = "James";
		friends[0].friendAv = false;
		friends[1].friendNam = "Abby";
		friends[1].friendAv = true;
		friends[2].friendNam = "Blake";
		friends[2].friendAv = true;
		friends[3].friendNam = "Jules";
		friends[3].friendAv = false;
		
		
		
		foreach (var i in friends)
		{
		Console.WriteLine( i.friendNam);
			if (i.friendAv == true)
		{	
		Console.WriteLine( " is Available.");
		} 
		else
			{
				Console.WriteLine( " is Unavailable.");
			}
		
		}
		
	}
}

public class friend
{ 
	public string friendNam = "Potion";
	public bool friendAv = true;
	
	
	
}
}
